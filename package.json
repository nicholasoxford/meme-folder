{
  "name": "mmfldr",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"npm run start\"",
    "start": "wrangler pages dev --compatibility-date=2023-10-16 ./public --r2=R2 --d1=DB",
    "local:migrate": "wrangler d1 migrations apply \"mmfldr-db\" --local",
    "typecheck": "tsc",
    "generate:types": "bunx supabase gen types typescript --project-id \"hofqjlvkztgcktmkgkxf\" --schema public > types/supabase.ts",
    "pages:deploy": "bun run build && wrangler pages deploy ./public"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-run/cloudflare": "^2.1.0",
    "@remix-run/cloudflare-pages": "^2.1.0",
    "@remix-run/css-bundle": "^2.1.0",
    "@remix-run/react": "^2.1.0",
    "@supabase/auth-helpers-remix": "^0.2.2",
    "@supabase/supabase-js": "^2.38.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "isbot": "^3.6.8",
    "lucide-react": "^0.288.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "unique-names-generator": "^4.7.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230518.0",
    "@remix-run/dev": "^2.1.0",
    "@remix-run/eslint-config": "^2.1.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "latest",
    "eslint": "^8.38.0",
    "tailwindcss": "latest",
    "typescript": "^5.1.0",
    "wrangler": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
